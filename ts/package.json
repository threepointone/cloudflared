    "yargs": "^17.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/yargs": "^17.0.33",
    "tsx": "^4.16.0",
    "typescript": "^5.4.0"
  }
}
