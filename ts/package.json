{
  "name": "cloudflared-ts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "TypeScript rewrite scaffold for cloudflared",
  "license": "MIT",
  "bin": {
    "cloudflared-ts": "dist/cli/index.js"
  },
  "scripts": {
